                </fragment>
                <fragment>
                    <string key="content">	•	</string>
                    <attributes>
                        <font key="NSFont" size="13" name="ArialMT"/>
                        <paragraphStyle key="NSParagraphStyle" alignment="justified" lineBreakMode="wordWrapping" baseWritingDirection="natural" paragraphSpacing="5" paragraphSpacingBefore="5" firstLineHeadIndent="36" headIndent="36" defaultTabInterval="36">
